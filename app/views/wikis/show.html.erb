<h1>Wiki#show</h1>

      
      <h1><%= @wiki.title %></h3>
      <p><%= @wiki.body %></p>
     
      <p>Created by <%= @wiki.user %></p>
      <p>Collaborators:
        <% unless @wiki.collaborators.empty? %>
        <% @wiki.collaborators.each do |collaborator| %>
        <p><%= collaborator.email %></p>
        <% end %>
        <% end %></p>
     
   <%= button_to 'Edit',  edit_wiki_url(@wiki.id), :method => :get, class: 'btn btn-success' %>
   
   
   <%=button_to "Delete",wiki_path(@wiki.id), :method => :delete,  class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this post?' } %>
